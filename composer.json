{
  "name": "rollun-com/rollun-datastore",
  "description": "Datastore",
  "license": "proprietary",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-expressive": "^1.0",
    "zendframework/zend-expressive-helpers": "^2.0",
    "zendframework/zend-expressive-fastroute": "^1.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-stdlib": "^3",
    "zendframework/zend-json": "^3",
    "zendframework/zend-db": "2.8.2",
    "zendframework/zend-expressive-twigrenderer": "^1.0",
    "zendframework/zend-http": "^2.5",
    "xiag/rql-parser": "^1.0",
    "symfony/filesystem": ">=2.7",
    "phpunit/phpunit": "^4.0",
    "psr/log": "^1.0",
    "rollun-com/rollun-installer": "^1.0",
    "mindplay/jsonfreeze": "0.3.3.x-dev",
    "rollun-com/rollun-logger": "^1.0",
    "rollun-com/rollun-actionrender": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "squizlabs/php_codesniffer": "^2.3",
    "filp/whoops": "^1.1 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\datastore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\datastore\\": "tests/src"
    }
  },
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "lib-install": "rollun\\installer\\Command::install",
    "lib-uninstall": "rollun\\installer\\Command::uninstall",
    "lib-reinstall": "rollun\\installer\\Command::reinstall"
  }
}
